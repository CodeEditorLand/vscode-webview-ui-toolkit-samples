{
	"name": "component-gallery-react",
	"displayName": "component-gallery-react",
	"description": "A sample extension to show off all React components in the Webview UI Toolkit for Visual Studio Code.",
	"main": "./out/extension.js",
	"scripts": {
		"build:webview": "cd webview-ui && npm run build",
		"compile": "tsc -p ./",
		"install:all": "npm install && cd webview-ui && npm install",
		"start:webview": "cd webview-ui && npm run start"
	},
	"contributes": {
		"commands": [
			{
				"command": "component-gallery-react.showGallery",
				"title": "Component Gallery (React): Show"
			}
		]
	},
	"dependencies": {},
	"devDependencies": {
		"@types/glob": "8.1.0",
		"@types/node": "22.13.4",
		"glob": "11.0.1"
	}
}
